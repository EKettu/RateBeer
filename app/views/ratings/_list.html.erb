<table class="table table-hover">
    <thead>
    <th></th>  
    <th>average</th>             
    </thead>
    <% items.each do |item| %>
        <tr>
            <td><%= link_to item.name, item %> </td>
            <td><%= round(item.average_rating) %></td>    
        </tr>
        <% end %>
</table>
